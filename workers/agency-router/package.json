{
  "name": "agency-router",
  "version": "1.0.0",
  "description": "Intelligent task router using skill graph and keyword matching",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "build": "tsc",
    "test": "node --loader tsx ./node_modules/.bin/vitest",
    "test:unit": "vitest src/**/*.test.ts",
    "test:integration": "vitest src/integration.test.ts",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "hono": "^4.0.11",
    "wrangler": "^3.28.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260218.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
